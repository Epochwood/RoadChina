      },
      "apply": {
        "model": "roadchina:thin_post_gray_part"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "roadchina:thin_post_gray_part",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "roadchina:thin_post_gray_part",
        "y": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "roadchina:thin_post_gray_part",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "roadchina:thin_post_gray_part",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "roadchina:thin_post_gray_part",
        "x": 90
      }
    }
  ]
}